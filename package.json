{
  "name": "hubot-irc",
  "version": "0.4.1",
  "author": "Fernando Ortiz <nandub@nandub.info>",
  "description": "IRC adapter for hubot",
  "keywords": "hubot irc adapter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/nandub/hubot-irc.git"
  },
  "bugs": {
    "url": "http://github.com/nandub/hubot-irc/issues"
  },
  "main": "./src/irc.coffee",
  "engine": "node > 10.19.0",
